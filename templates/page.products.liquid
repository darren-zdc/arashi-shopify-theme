<div id="vehicle-display"></div>



<script type="application/javascript">
  var skuVehiclesUrl = "{{ 'sku-vehicles.json' | asset_url }}";
  const params = new URLSearchParams(window.location.search);
  const vehicleParam = params.get('vehicle');

  if (vehicleParam) {
      fetch(skuVehiclesUrl)
          .then(response => response.json())
          .then(data => {
              const matchingSkus = data.filter(item => item.vehicle === vehicleParam);
              console.log(matchingSkus); // Process your data here
              // You can now use 'matchingSkus' to display information or further process data
              document.getElementById('vehicle-display').textContent = matchingSkus;
          })
          .catch(error => console.error('Error loading SKU vehicles data:', error));
  }
</script>