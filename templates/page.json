        "custom_liquid": "<!-- Access vehicle metafields -->\n{% assign vehicles = shop.metaobjects.vehicle %}\n\n{% assign vehicles = shop.metaobjects.vehicle %}\n{% for vehicle in vehicles .values%}\n  <p>Make: {{ vehicle.model.value }}<\/p>\n{% endfor %}\n\n\n<!-- Create Make dropdown -->\n<label for=\"make\">Make:<\/label>\n<select name=\"make\" id=\"make\">\n  {% for vehicle in vehicles.values %}\n    <option value=\"{{ make }}\">{{ vehicle.make }}<\/option>\n  {% endfor %}\n<\/select>\n\n<!-- Create Model dropdown -->\n<label for=\"model\">Model:<\/label>\n<select name=\"model\" id=\"model\">\n  {% for vehicle in vehicles.values %}\n    <option value=\"{{ model }}\">{{ vehicle.model }}<\/option>\n  {% endfor %}\n<\/select>\n\n<!-- Create Year dropdown -->\n<label for=\"year\">Year:<\/label>\n<select name=\"year\" id=\"year\">\n  {% for vehicle in vehicles.values %}\n    <option value=\"{{ year }}\">{{ vehicle.year }}<\/option>\n  {% endfor %}\n<\/select>\n\n<!-- Create Model Number dropdown -->\n<label for=\"model_number\">Model Number:<\/label>\n<select name=\"model_number\" id=\"model_number\">\n  {% for vehicle in vehicles.values %}\n    <option value=\"{{ modelNumber }}\">{{ vehicle.model_number }}<\/option>\n  {% endfor %}\n<\/select>\n\n<!-- Create Type dropdown -->\n<label for=\"type\">Type:<\/label>\n<select name=\"type\" id=\"type\">\n  {% for type in vehicleMeta.type %}\n    <option value=\"{{ type }}\">{{ type }}<\/option>\n  {% endfor %}\n<\/select>",
